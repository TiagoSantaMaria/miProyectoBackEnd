<div>
    {{#each response.docs}}
        <hr>
            <li>Nombre Producto: {{this.title}}</li>
            <li>Precio: {{this.price}}</li>
            <li>Stock: {{this.stock}}</li>
            <button onclick="agregarAlCarrito('{{this._id}}','{{this.title}}','{{this.price}}')">AGREGAR PRODUCTO AL CARRITO</button>
        </hr>
    {{/each}}
    <div>
        <button><a href="{{response.prevLink}}"> PREV PAGE</a></button>
        <button><a href="{{response.nextLink}}"> NEXT PAGE</a></button>
    </div>
    
</div>
<script src="/socket.io/socket.io.js"></script>
<script src="/js/home.js"></script>